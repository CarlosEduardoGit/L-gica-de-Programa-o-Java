
public class TESTE {
	public static void main(String[] args) {
		System.out.println("Olá Devs!\nOlá Devs");
		System.out.println("Olá Devs!\rOlá Devs");
		System.out.println("Olá Devs!\bOlá Devs");
		System.out.println("Olá Devs!\tOlá Devs");
		System.out.println("Olá Devs!\fOlá Devs");
		System.out.println("Olá Devs!\'Olá Devs");
	}
}
