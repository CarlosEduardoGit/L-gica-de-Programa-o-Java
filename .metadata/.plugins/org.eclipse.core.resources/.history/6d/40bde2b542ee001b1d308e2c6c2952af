
public class CaracteresEspeciais {

	public static void main(String[] args) {
		
		/*
		|---------------------------------------|
		|Representação |      Significado       |
		|---------------------------------------|
		|    \n        | Pular linha            |
		|--------------|------------------------|
		|    \r	       | Retorno de Carro       |
		|--------------|------------------------|
		|    \b        | Retrocesso             |
		|--------------|------------------------|
		|    \t        | Tabulação              |
		|--------------|------------------------|
		|    \f        | Nova página            |
		|--------------|------------------------|
		|    \'        | Apóstrofe              |
		|--------------|------------------------|
		|    \"        | Aspas                  |
		|--------------|------------------------|
		|    \\        | Barra Invertida        |
		|--------------|------------------------|
		|    \u223d    | Caractere Unicode 233d |
		|--------------|------------------------|
		|    \g37      | Octal 37               |
		|--------------|------------------------|
		|    \fca      | Hexadecimal FCA        |
		|---------------------------------------|
		
		*/


	}

}



